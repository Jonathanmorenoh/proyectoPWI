        <div class="container">
            <div class="row">
                <div class="col-12" style="justify-content-center;">
                    <h3>Modificacion de usuarios.</h3>
                    <div class="col-4 mt-3">
                    <a class="site-btn mt-4 mb-4" href="/admin/usuarios/alta">Alta de Usuario</a>
                    {{! admin/productos/alta}}
                </div>
                    <table class="table table-hover table-responsive mt-3 mb-3">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Nombre y apellido</th>
                                <th>Usuario</th>
                                <th>DNI</th>
                                <th>E-mail</th>
                                <th>Admin</th>
                                <th></th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each usuarios}}
                            <tr>
                                <td>{{id}}</td>
                                <td>{{nombre}} {{apellido}}</td>
                                <td>{{usuario}}</td>
                                <td>{{dni}}</td>
                                <td>{{correo}}</td>
                                <td>{{admin}}</td>
                                <td><a href="/admin/usuarios/modificar/{{id}}"><i class="fas fa-cogs pr-2 pl-2" style="cursor: pointer;"></i></a></td>
                                <td><a href="/admin/usuarios/baja/{{id}}"><i class="fas fa-trash text-danger pr-2 pl-2" style="cursor: pointer;"></i></a></td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
</div>